<p id="notice"><%= notice %></p>

<h1>Billboards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @billboards.each do |billboard| %>
    
    <P>
        <%= billboard.title %>
        <%= billboard.country %>
        <%= link_to 'Show', billboard %>
    <% end %>
    </P>
  </tbody>
</table>

<br>
<%= link_to 'New Billboard', new_billboard_path %>
